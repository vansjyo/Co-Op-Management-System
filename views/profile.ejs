   <!DOCTYPE html> 
   <html>
   <head>

   	<title>PROFILE | ONLINE DELIVERY</title>

   	<meta charset="UTF-8">
   	<meta name="viewport" content="width=device-width" initial-scale="1.0">
   	<meta name="description" content="online coop service nitk">
   	<meta name="keywords" content="nitk online coop">

   	<link rel="shortcut icon" href="/assets/images/favicon.ico" type="image/x-icon" />

   	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.3/css/materialize.min.css">
   	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
   	<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Suez+One">
   	<link rel="stylesheet" href="/assets/css/flexslider.css" type="text/css">
   	<link rel="stylesheet" href="/assets/css/style.css">
   	<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">


   </head>

   <body id="back">

   	<!-- header  -->
   	<% include partials/header.ejs %>
   	<!-- ////header -->

   	<!-- side nav menu -->
   	<ul id="slide-out" class="side-nav">
   		<li class="white">
   			<div class="userView">
   				<div class="background">
   					<img src="/assets/images/nitklogo.jpg" width="100%">
   				</div>
   				<a href="https://www.nitk.ac.in"><pre>Nitk Surathkal   www.nitk.ac.in</pre></a>
   			</div>
   		</li>
       <li><a href="/home" class="waves-effect space">HOME</a></li>
       <li><a href="/profile" class="waves-effect space">PROFILE</a></li>
       <li><div class="divider"></div></li>
       <li><a href="#services" class="waves-effect space">SERVICES</a></li>
       <li><a href="/contact" class="waves-effect space">CONTACT</a></li>
       <li><a href="/categories" class="waves-effect space">SHOP NOW</a></li>
       <li><div class="divider"></div></li>
       <li><a class="waves-effect space" href="/help">HELP</a></li>
       <li><a href="/logout" class="waves-effect space">LOGOUT</a></li>
     </ul>
     <!-- ////side nav menu -->

     <main>


       <br><br><br>

       <!-- center image -->
       <div class="div3"><img src="/assets/images/back.jpg" id="backimg" width="100%" style="display: none"></div>
       <!-- ///center image -->




       <div class="container">
         <div class="row center">
         <div class="col s12 m6 teal lighten-1 z-depth-4 profile pad30"> 
           <form action="" method="post">
             <div class="center"><i class="material-icons large pad30">account_circle</i></div><br>
             <div class="row">
               <div class="input-field col s6">
                 <input type="text" name="firstname" class="validate center" required value="<%= user.local.firstname %>">
               </div>
               <div class="input-field col s6">
                 <input type="text" name="lastname" class="validate center" required value="<%= user.local.lastname %>">
               </div>
             </div> 
             <div class="row">
              <div class="input-field col s12">
                <input name="telephone" type="tel" maxlength="12" class="validate center" required value="<%= user.local.telephone %>"> 
              </div>
            </div><br>
           <button class="btn waves-effect waves-light" type="submit">EDIT INFO
            <i class="material-icons right">send</i>
          </button>
        </form>
        <br>
      </div>
    </div>

      <div class="row teal lighten-1 pad30 margin70 z-depth-4 profile"> 
        <h4 class="thicker white-text">Here are your previous orders</h4>
          <table class="striped highlight">
            <thead>
            <tr>
              <th>Item Name</th>
              <th>Item Price</th>
              <th>Item Quantity</th>
            </tr>
          </thead>

          <%user.local.orderList.forEach(order => {%>
            <tr>
              <td><%= order.item_name %></td>
              <td>Rs.<%= order.item_price %></td>
              <td>quantity:<%= order.item_quantity %></td>
            </tr>
          <% });%>
        </table>
        <br>
      </div>

    </main>

    <!-- footer -->
    <% include partials/footer.ejs %>
    <!-- ////footer -->


    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>           
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.3/js/materialize.min.js"></script> 
    <script src="/assets/js/jquery.flexslider.js"></script>
    <script src="/assets/js/main.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js"></script>

  </body>
  </html>